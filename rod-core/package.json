		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.15",
		"@types/express": "^4.17.20",
		"@types/node": "^20.8.9",
		"@types/uuid": "^9.0.3",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"esbuild": "^0.19.5",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-prettier": "^5.0.1",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.3",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	}
