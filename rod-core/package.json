{
	"name": "rod-core-server",
	"version": "0.0.1",
	"description": "Core server for the RESTful operations of botanist",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "nodemon --watch \"*.ts\" --exec \"ts-node\" ./src/index.ts",
		"type-check": "tsc --noEmit",
		"lint": "eslint ./src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix",
		"build": "sh ./scripts/build.sh"
